lm.tcrit <- lm(log10(tcrit)~n.mat, data=df.tcrit)
df.p <- data.frame(n.mat=seq(11,99,11/4),
tcrit=10^(predict(lm.tcrit, data.frame(n.mat=seq(11,99,11/4)))))
plt.log <- ggplot(df.tcrit, aes(x=n.mat, y=log10(tcrit))) +
geom_point() +
geom_line(data=df.p) +
theme_minimal() +
coord_cartesian(xlim=c(11,99), ylim=c(-1,2.5)) +
scale_x_continuous(breaks=seq(11,99,11)) +
xlab('Number of parameters') +
ylab('Critical simulation time (log s)')
print(plt)
ggsave('C:/Users/47873315B/OneDrive - Generalitat de Catalunya/PhD/docs/CCIA2023/figs/crit_times_log.pdf', plt.log, units='px', height=1000, width = 2000)
plt.exp <- ggplot(df.tcrit, aes(x=n.mat, y=tcrit)) +
geom_point() +
geom_line(data=df.p) +
theme_minimal() +
scale_x_continuous(breaks=seq(11,99,11)) +
xlab('Number of parameters') +
ylab('Critical simulation time (s)')
print(plt.exp)
ggsave('C:/Users/47873315B/OneDrive - Generalitat de Catalunya/PhD/docs/CCIA2023/figs/crit_times.pdf', plt.exp, units='px', height=1000, width = 2000)
